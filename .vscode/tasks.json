{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Start client",
			"type": "shell",
			"command": "cd client && npm start",
			"group": "build",
			"isBackground": true,
			"problemMatcher": [
				"$tsc"
			]
		},
		{
			"label": "Start API",
			"type": "shell",
			"command": "dotnet run",
			"group": "build",
			"isBackground": true,
			"options": {
				"cwd": "${workspaceFolder}/api/AzDevOpsApi"
			},
			"problemMatcher": []
		}
	]
}